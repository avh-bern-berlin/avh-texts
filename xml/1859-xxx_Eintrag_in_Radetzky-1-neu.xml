    <text>
        <front/>
        <body>
            <pb n="49" facs="#f0001"/>
            <lg type="poem">
                <l>Von des Lebens Gütern allen</l>
                <lb break="yes"/>
                <l>I&#x017F;t der Ruhm das Höch&#x017F;te doch;</l>
                <lb break="yes"/>
                <l>Wenn der Leib in Staub zerfallen</l>
                <lb break="yes"/>
                <l>Bleibt der große Name noch.</l>
            </lg>
            <lb break="yes"/>
            <p>
                <hi rendition="#et">(Schiller, das Siegesfe&#x017F;t.)</hi>
            </p>
            <lb break="yes"/>
            <closer>
                <salute>
                    <hi rendition="#et">Den edlen grei&#x017F;en Helden Feldmar&#x017F;chall<lb break="yes"/>Grafen Radetzky begrüßt ehrfurchtsvoll<lb break="yes"/>
                        <hi rendition="#g">Alexander v. Humboldt.</hi>
                    </hi>
                </salute>
            </closer>
            <lb break="yes"/>
        </body>
        <back/>
    </text>
